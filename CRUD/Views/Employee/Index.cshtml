@model List<CRUD.Models.Domain.Employee>
@{
}
<h1>Employees</h1>
<table class=" table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>Date OF Birth</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employees in Model)
        {
            <tr>
                <td>@employees.Id</td>
                <td>@employees.Name</td>
                <td>@employees.Email</td>
                <td>@employees.Salary</td>
                <td>@employees.DateOfBirth.ToString("dd-MM-yyyy")</td>
                <td>@employees.Department</td>
                <td><a class="btn btn-sm btn-warning" asp-controller="Employee" asp-action="Edit" asp-route-id="@employees.Id">Edit</a></td>
                
            </tr>
            
        }
    </tbody>

</table>